package java_21_10_07;

public class Ex11_ForEx {

	public static void main(String[] args) {
		// 어떤 정수를 하나 입력받아서
		// 1부터 입력받은 정수까지의 숫자 중에서
		// 1의배수, 3의배수를 제외한 숫자의 총합을 구하시오.
		
		int num = 0;
		for(int i=1; i<=20; i++ ) {
			if(i % 3 == 0) {
				System.out.println(i);
			}else {
				System.out.println(num);
			}
			
		}

	}

}
